list1=['red','orange','mango','4','5']
list1.append('strawberry')
print(list1)
list1.remove('mango')
print(list1)
list1[0]='28'
print("upddated list",list1)

#dictionary
dict1={1:'cse',2:'ece',3:'eee',4:'mech'}
print(dict1)
dict1['2']='civil'
print(dict1)
dict1['0']='ai&ds'
print("updated dictionary",dict1)

#set
set1={1,4,6,7,23,45}
set1.add(78)
print(set1)
set1.remove(6)
print(set1)
set1.discard(4)
print("updated set",set1)
